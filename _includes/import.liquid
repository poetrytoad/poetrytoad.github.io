{% assign pp = include.arg %}
{% if pp.import %}
    {% if pp.from %}
        {% assign q=site.data.[pp.from].[pp.import] %}
        {% if pp.swap %}
            {% assign swap=true %}
        {% endif %}
    {% else %}
        {% assign q=site.data.[page:collection].[pp.import] %}
        {% assign swap=false %}
    {% endif %}
    {% if q.hidden %}
        {% assign l=false %}
    {% else %}
        {% assign l=pp.import %}
    {% endif %}
    {% assign texpand=page.expand.[pp.import] %}
{% else %}
    {% assign q=pp %}
    {% assign l=false %}
    {% assign swap=false %}
    {% assign texpand=page.expand.[pp.name] %}
{% endif %}
